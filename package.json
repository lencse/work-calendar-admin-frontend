{
  "name": "work-calendar-admin-frontend",
  "version": "1.0.0",
  "description": "Work calendar admin frontend",
  "main": "index.js",
  "license": "MIT",
  "author": "Levente LÃ¶ki <leventeloki@gmail.com> (https://lokilevente.hu/)",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lencse/work-calendar-admin-frontend.git"
  },
  "scripts": {
    "build-ci": "rm -rf ci/bin && tsc -p ./ci/src --outDir ./ci/bin",
    "write-config-file": "node ci/bin/build-config.js",
    "clean": "rm -rf build dist",
    "build-typescript": "tsc -p ./src --outDir ./build",
    "build-webpack": "webpack --colors --progress --config ./ci/bin/webpack.config.js",
    "build": "yarn run clean && yarn build-webpack-monitor && yarn build-typescript && yarn build-webpack",
    "watch-typescript": "tsc -p ./src --outDir ./build --watch",
    "watch-webpack": "webpack-dev-server --config ./ci/bin/webpack.config.js --port 8211",
    "watch": "yarn run clean && (yarn watch-typescript & yarn watch-webpack)",
    "postinstall": "yarn build-ci && yarn write-config-file",
    "lint": "tslint -c tslint.json src/**/*.ts",
    "test": "yarn lint && if [ ! -f ./dist/admin-frontend/index.html ]; then echo \"index.html is missing\"; exit 1; fi; if [ ! -d ./dist/admin-frontend/js ]; then echo \"dist/admin-frontend/js is missing\"; exit 1; fi; if [ ! -d ./dist/admin-frontend/css ]; then echo \"dist/admin-frontend/css is missing\"; exit 1; fi; if [ ! -d ./dist/admin-frontend/images ]; then echo \"dist/admin-frontend/images is missing\"; exit 1; fi; find -L dist",
    "build-webpack-monitor": "mkdir dist && yarn copy-webpack-monitor",
    "copy-webpack-monitor": "cp -R node_modules/webpack-monitor/build dist/webpack-monitor && sed -i -e 's/\\/getstats/stats.json/g' dist/webpack-monitor/app.js"
  },
  "devDependencies": {
    "@types/colors": "^1.1.3",
    "@types/command-line-args": "^4.0.1",
    "@types/core-js": "^0.9.42",
    "@types/lodash": "^4.14.74",
    "@types/node": "^8.0.24",
    "@types/react": "^16.0.5",
    "@types/react-mdl": "^1.7.22",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-preset-env": "^1.6.0",
    "colors": "^1.1.2",
    "command-line-args": "^4.0.7",
    "copy-webpack-plugin": "^4.0.1",
    "css-loader": "^0.28.7",
    "cssnano": "^3.10.0",
    "extract-text-webpack-plugin": "^3.0.0",
    "html-webpack-plugin": "^2.30.1",
    "node-sass": "^4.5.3",
    "optimize-css-assets-webpack-plugin": "^3.1.1",
    "sass-loader": "^6.0.6",
    "style-loader": "^0.18.2",
    "tslint": "^5.7.0",
    "typescript": "^2.4.2",
    "uglifyjs-webpack-plugin": "^0.4.6",
    "webpack": "^3.5.5",
    "webpack-dev-server": "^2.7.1",
    "webpack-monitor": "^1.0.13"
  },
  "dependencies": {
    "12factor-config": "^1.3.1",
    "lencse-getmdl-dashboard": "^1.0.3",
    "lodash": "^4.17.4",
    "react": "^15.6.1",
    "react-click-outside": "^3.0.0",
    "react-dom": "^15.6.1",
    "react-mdl": "^1.10.3",
    "react-mdl-selectfield": "^0.2.0",
    "ts-jsonapi": "^2.1.0"
  }
}
